<?xml version="1.0"?>
<package >
  <metadata>
    <id>JetBrains.ReSharper.Plugins.XamlAttributeOrdering</id>
    <version>$version$</version>
    <title>Xaml Attribute Ordering Plugin</title>
    <authors>Jake Ginnivan</authors>
    <owners>Jake Ginnivan</owners>
    <licenseUrl>https://github.com/JakeGinnivan/XamlAttributeOrderingCodeCleanup/blob/master/LICENSE</licenseUrl>
    <projectUrl>https://github.com/JakeGinnivan/XamlAttributeOrderingCodeCleanup</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Adds an option to Xaml code cleanups which will order Attributes into logical and configurable groups</description>
    <releaseNotes>Support for ReSharper 8.1</releaseNotes>
    <copyright>Copyright Jake Ginnivan 2013</copyright>
    <tags>Xaml Formatting CodeCleanup</tags>
    <dependencies>
      <dependency id="ReSharper" version="8.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\XamlAttributeOrdering.8.0.dll"
          target="ReSharper\v8.0\plugins\" />
    <file src="bin\$configuration$\XamlAttributeOrdering.8.1.dll"
          target="ReSharper\v8.1\plugins\" />
  </files>
</package>
